{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "northwindservice",
        "image": "gmatysik/northwindservice",
        "hostname": "northwindservice",
        "essential": true,
        "memory": 128
      },
      {
        "name": "northwindmvc",
        "image": "gmatysik/northwindmvc",
        "hostname": "northwindmvc",
        "essential": false,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": [
          "northwindservice"
        ]
      }
    ]
  }
  